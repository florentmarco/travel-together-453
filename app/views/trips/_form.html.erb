<div class="container">
  <%= simple_form_for @trip do |f| %>
    <%= f.input :name %>
    <%= f.input :location %>
    <%= f.input :start_date, input_html: {class: "datepicker"} %>
    <%= f.input :end_date, input_html: {class: "datepicker"} %>
    <%= f.submit %>
  <% end %>

  <%= link_to 'Back', trips_path %>
</div>
